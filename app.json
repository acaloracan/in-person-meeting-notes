{
  "expo": {
    "name": "in-person-meeting-notes",
    "slug": "in-person-meeting-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "inpersonmeetingnotes",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone."
      },
      "bundleIdentifier": "com.anonymous.inpersonmeetingnotes"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.inpersonmeetingnotes",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "./plugins/google-services.config.js",
        {
          "source": "./android/app/google-services.json",
          "dest": "android/app/google-services.json"
        }
      ],
      [
        "./plugins/audio-recording.config.js",
        {
          "iosMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone.",
          "androidChannelId": "recording",
          "androidChannelName": "Recording",
          "androidChannelDescription": "Foreground service for microphone recording"
        }
      ],
      "expo-sqlite",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "27c6def9-7f01-4fc3-addb-01368048e0c5"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "kyuseishus-organization"
  }
}
